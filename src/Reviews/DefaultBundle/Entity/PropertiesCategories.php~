<?php

namespace Reviews\DefaultBundle\Entity;

/**
 * PropertiesCategories
 */
class PropertiesCategories
{
    /**
     * @var integer
     */
    private $propertyCategoryId;

    /**
     * @var string
     */
    private $name;


    /**
     * Get propertyCategoryId
     *
     * @return integer
     */
    public function getPropertyCategoryId()
    {
        return $this->propertyCategoryId;
    }

    /**
     * Set name
     *
     * @param string $name
     *
     * @return PropertiesCategories
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }
}
